#Include 'Protheus.ch'

User Function LPB07CRD()

/*
IIF(SRZ->RZ_CC$"2110,2204,2303,3108,3304,4105",213010004,213010001)
IIF(SRZ->RZ_CC$"2110,2204,2303,3108,3304,4105",115030002,115030001)
*/

	local cConta
	
	
	IF ALLTRIM(SRZ->RZ_CC) = "2110" .OR. ALLTRIM(SRZ->RZ_CC) = "2204" .OR. ALLTRIM(SRZ->RZ_CC) = "2303" .OR. ALLTRIM(SRZ->RZ_CC) = "3108" .OR. ALLTRIM(SRZ->RZ_CC) = "3304" .OR. ALLTRIM(SRZ->RZ_CC) = "4105" .OR. ALLTRIM(SRZ->RZ_CC) = "4302"
		cConta := "115030002"
		
	ELSE
		cConta := "115030001"		
	
	ENDIF
	


Return cConta

