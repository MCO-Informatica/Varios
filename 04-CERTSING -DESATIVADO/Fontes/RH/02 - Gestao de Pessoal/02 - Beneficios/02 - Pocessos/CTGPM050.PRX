nDFaltas    :=  mv_par13
nDFerias    :=  mv_par14  // 1-Sim; 2-Nao
nCriterio	:=  mv_par15
nFerProg    :=  mv_par16
nQtdDed		:=  mv_par17
nSalIncor	:=  mv_par18   
cCodigos    :=  AllTrim(mv_par19)+AllTrim(mv_par20)+AllTrim(mv_par21)+AllTrim(mv_par22)
