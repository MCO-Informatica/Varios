AaDd(aProceed,{ 'Filial',;
                'Matricula',;
                'Nome',;
                'Centro de Custo',;
                'Periodo Compra',;
                'Qtde. Beneficios',;
                'Qtde. Faltas',;
                'Per.Inicio Ponto',; 
                'Per.Final Ponto',;
                'Dias Afastado',;
                'Dias Ferias',;
                'Valor Unitario',;
                'Valor Total',;
                'Codigo Beneficio',;
                'Descricao Beneficio';
                 })


//-> Procura primeiro funcionario                                 
dbSelectArea( "SRA" )
