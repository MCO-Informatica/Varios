														%>
															<a href="" class="texto">
																<b>
																	<%=HttpSession->aNewPares[nZ][nI]:CDESCRIPTION%>
																	<%=IIf(HttpSession->cTpAval == "3", " - "+HttpSession->aNewPares[nZ][nI]:cProjectDescription,Nil)%>
																</b>
