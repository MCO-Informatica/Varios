							                      			      .And. Date() > HttpSession->aNewPares[nZ][nI]:dAnswerFinalDate %>
															<%=STR0017%><!--Atrasado-->
														<% Else %>
															<%=HttpSession->aNewPares[nZ][nI]:cEvaluateStatusDescription%>
						                          		<% EndIf %>
