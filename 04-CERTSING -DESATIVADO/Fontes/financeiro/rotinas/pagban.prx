#INCLUDE "RWMAKE.CH"

/////  PROGRAMA PARA SEPARAR O BANCO DO FORNECEDOR
//// PAGFOR - POSICOES ( 96 - 98 ) 

USER FUNCTION PAGBAN()
dbselectarea("SA2")

_BANCO  :=  SUBSTR(SE2->E2_CODBAR,1,3)

IF SUBSTR(SE2->E2_CODBAR,1,3) == "   "
   _BANCO  :=  POSICIONE("SA2",1,XFILIAL("SA2")+SE2->E2_FORNECE+SE2->E2_LOJA,"SA2->A2_BANCO")
Else
   _BANCO := "000"
EndIf

return(_BANCO)
