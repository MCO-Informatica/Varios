		Else
			dbSelectArea("SUS")
			dbSetOrder(1)
			dbSeek(xFilial()+SUA->UA_CLIENTE+SUA->UA_LOJA)
			MaFisIni(SUS->US_COD,SUS->US_LOJA,"C","N",SUS->US_TIPO,MaFisRelImp("MTR700",{"SUA","SUB"}),,,"SB1","MTR700")
		EndIf
		
		DbSelectArea("SUB")
