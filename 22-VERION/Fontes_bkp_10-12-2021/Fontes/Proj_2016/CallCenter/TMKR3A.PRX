_cNrVrOrf := SUA->UA_NUM
While !Eof() .And. SUA->UA_NUM = _cNrVrOrf     //xFilial("SUA") == SUA->UA_FILIAL .And. //SUA->UA_NUM <= _cNrVrOrf
		_cObs := Alltrim(SUA->UA_VEROBS)

