User Function GVRESTOQ()
_aArea     := GetArea()
_cVrPrdSld := TMP1->CK_PRODUTO
_cVrPrdLcl := TMP1->CK_LOCAL
_nSldAtual := 0
DbSelectArea("SB2")
DbSetOrder(1)
If DbSeek(Xfilial("SB2")+_cVrPrdSld+_cVrPrdLcl)
   _nSldAtual := (SB2->B2_QATU-(SB2->B2_RESERVA+SB2->B2_QEMP))
Endif

RestArea(_aArea)
Return(_nSldAtual)
