//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ


Private _VLunit
_VLunit := 0
dbSelectArea("SB1")
dbSetOrder(1)
Dbseek(Xfilial("SB1") + ACOLS[N,2])
IF B1_UM == 'KT'
	
	dbSelectArea("SG1")
	dbSetOrder(1)
	Dbseek(Xfilial("SG1") + ACOLS[N,2])
	While !Eof() .and. ACOLS[N,2] = SG1->G1_COD
		dbSelectArea("SB1")
		dbSetOrder(1)
		Dbseek(Xfilial("SB1") + SG1->G1_COMP)
		_VLunit := _VLunit + SB1->B1_VERVEN
		dbSelectArea("SG1")
		dbskip()
	EndDo
	
ELSE
	
	_VLunit := SB1->B1_VERVEN
	
ENDIF


Return(_VLunit)
