	AADD(aCampos,{"TELEVEND"     ,"C",15,0})
	AADD(aCampos,{"NATUREZA"    ,"C",10,0})
	AADD(aCampos,{"PREFIXO"         ,"C",TamSx3('E2_PREFIXO')[1],0})
