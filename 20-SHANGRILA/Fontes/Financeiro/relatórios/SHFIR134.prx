						Endif
					Next
					If !lChkDRef
						MsgStop(OemToAnsi(STR0036)) //"Uma das datas deve coincidir com a Data de Referencia"
						Loop
					Endif
				Endif

				If lPerg
					dData01 := mv_par01
					dData02 := mv_par02
