				EndIf
			Else
				Replace VALOR12   With VALOR12 + nSldTit
			EndIf

			nValor := VALOR00+VALOR01+VALOR02+VALOR03+VALOR04+VALOR05+VALOR06
			nValor += VALOR12+VALOR11+VALOR10+VALOR09+VALOR08+VALOR07
			Replace SALDOFIM  With nValor
