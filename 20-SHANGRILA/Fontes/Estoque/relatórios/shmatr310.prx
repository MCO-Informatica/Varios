		 Aadd( aHelpSpa, "impuestos del valor en el archivo de    " )
 		 Aadd( aHelpSpa, "Items de la Factura de Entrada (SD1)    " )
 		 
         PutSX1Help("P.MTR31014.",,,aHelpSpa)
	  Endif  
      DBSKIP()
   ENDDO                                                 
   DBCOMMITALL()
endif

