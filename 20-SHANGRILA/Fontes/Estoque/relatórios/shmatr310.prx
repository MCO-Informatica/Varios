		EndIf
		@ li,199 PSay nAt4 PicTure tm(nAt4,14)
		If nAt2 != 0
			@ li,214 PSay (100*nAt4/nAt2)-100 PicTure "9999.9"
		EndIf
		
		li += 2
		nAg1	+= nAt1
		nAg1Cus	+= nAt1Cus
		nAg2	+= nAt2
		nAg3	+= nAt3
		nAg4	+= nAt4
