User Function MT094FIL()

Local cRet := ""
Local cCodUser  := RETCODUSR()

IF cCodUser $ "000015" //MATEUS.SANCHEZ
    cRet := "CR_TIPO  == 'SC'"
ELSE
    cRet := "CR_TIPO  <> ''"
ENDIF

Return (cRet)
