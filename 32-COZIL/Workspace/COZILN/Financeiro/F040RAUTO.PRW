User Function F040RAUTO()
Local aParam  := PARAMIXB
Local aAux    := PARAMIXB[1]
Local lLiquid := FunName() == "FINA460"

apMsgInfo("Passei aqui","F040RAUTO")

If lLiquid
	cMsg  := "Vendedor"+ FO2->FO2_VEND1 + CRLF
	cMsg  += "Pedido"+ FO2->FO2_PEDIDO + CRLF
	apMsgInfo(cMsg,"F040RAUTO")
	//Vendedor
	aAdd(aAux,{"E1_VEND1",M->FO2_VEND1, Nil})
	//Pedido
	aAdd(aAux,{"E1_VEND1",M->FO2_PEDIDO, Nil})
Endif

Return aAux

 
 