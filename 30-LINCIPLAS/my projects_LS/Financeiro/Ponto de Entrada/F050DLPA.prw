#INCLUDE "rwmake.ch"
#INCLUDE "PROTHEUS.CH"

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// PA na exclusão de contas a pagar - se for PA, apaga a movimentação financeira da inclusão do PA
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
User Function F050DLPA()
////////////////////////

If SE2->E2_TIPO = 'PA'
	RecLock('SE5',.f.)
	DbDelete()        
	MsUnLock()
EndIf
Return(.t.)

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// PA na exclusão de contas a pagar - se for PA, apaga a movimentação financeira da exclusão do PA
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
User Function F050EXCPA()
////////////////////////

If SE2->E2_TIPO = 'PA'
	DbDelete()
EndIf
Return()
