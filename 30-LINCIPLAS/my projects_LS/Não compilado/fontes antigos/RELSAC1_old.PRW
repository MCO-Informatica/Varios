Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nOrdem


//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ SETREGUA -> Indica quantos registros serao processados para a regua ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู


	_cQuery := "SELECT ZR_CODIGO, ZR_QUANT, ZR_CLIENTE, ZR_LOJA, ZR_NOME, "
	_cQuery += _cEnter + " 		ZR_PRODUTO, ZR_QUANT, ZR_EDICOES, ZR_PERIOD, ZR_INICIO, "
	_cQuery += _cEnter + " 		 ZR_TERMINO, ZR_OBS "
	_cQuery += _cEnter + " FROM " + RetSqlName('SZR') + " SZR WITH(NOLOCK) "
	_cQuery += _cEnter + " WHERE ZR_CLIENTE BETWEEN '" +DTOS(mv_par01) + "' AND '" + DTOS(mv_par02) + "' "
//	_cQuery += _cEnter + " AND ZR_CODIGO BETWEEN '" +DTOS(mv_par03) + "' AND '" + DTOS(mv_par04) + "' "
	_cQuery += _cEnter + " AND ZR_PRODUTO = '" + MV_PAR03 + "' "
	_cQuery += _cEnter + " AND ZR_PROXIMA BETWEEN '" +DTOS(mv_par04) + "' AND '" + DTOS(mv_par05) + "'"
//	_cQuery += _cEnter + " AND ZR_TERMINO BETWEEN '" +DTOS(mv_par06) + "' AND '" + DTOS(mv_par07) + "'"
	_cQuery += _cEnter + " AND SZR.D_E_L_E_T_ = '' "
                                                                                                
dbUseArea(.T., "TOPCONN", TCGenQry(,,_cQuery), 'TRB', .F., .T.)

TcSetField('TRB','ZR_CODIGO','C',0)
            
count to _nLastRec
SetRegua(_nLastRec)

dbGoTop()
While !EOF()
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica o cancelamento pelo usuario...                             ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	If lAbortPrint
		@nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
		Exit
	Endif
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Impressao do cabecalho do relatorio. . .                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	If nLin > 55 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
		Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
		nLin := 8
	Endif
	
	IF MV_PAR08==2
		@++nLin,00 PSAY TRB->ZR_CODIGO + ' ' + TRB->ZR_QUANT + '  ' + TRB->ZR_CLIENTE + '  ' + TRB->ZR_LOJA + '  ' + TRB->ZR_NOME + '  ' + TRB->ZR_PRODUTO + '  ' + TRB->ZR_EDICOES + '  ' + TRB->ZR_PERIOD + '  ' + DTOS(TRB->ZR_INICIO) + '  ' + DTOS(TRB->ZR_TERMINO) + '  ' + TRB->ZR_OBS
		DbSkip()
		IncRegua()		
	ELSE
		
		@++nLin,00 PSAY TRB->ZR_QUANT + '  ' + TRB->ZR_EDICOES + '  ' + TRB->ZR_PERIOD + '  ' + TRB->ZR_INICIO + '  ' + TRB->ZR_TERMINO
		
		_nTotal := 0
		_cAss := TRB->ZR_CODIGO
		
		DO WHILE _cAss == TRB->ZR_CODIGO .AND. !EOF()
			
			//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
			//ณ Verifica o cancelamento pelo usuario...                             ณ
			//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
			
			If lAbortPrint
				@nLin,01 PSAY "*** CANCELADO PELO OPERADOR ***"
				Exit
			Endif
			
			//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
			//ณ Impressao do cabecalho do relatorio. . .                            ณ
			//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
			
			If nLin > 55 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
				Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
				nLin := 8
			Endif
			
			@ ++nLin,11 PSAY TRB->ZR_CODIGO + '  ' + TRB->ZR_PRODUTO + '  '
			
			_nTotal += TRB->ZR_CODIGO
			
			
			dbSkip()
			IncRegua()
		EndDo
		
		@ ++nLin,130 PSAY "Total de Assinaturas: " + Trans(_nTotal,'@E 999,999,999.99')
		@ ++nlin,00 pSay __PrtThinLine()
		
	ENDIF
	
EndDo





dbClosearea()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Finaliza a execucao do relatorio...                                 ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SET DEVICE TO SCREEN

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

If aReturn[5]==1
	dbCommitAll()
	SET PRINTER TO
	OurSpool(wnrel)
Endif

MS_FLUSH()

Return
