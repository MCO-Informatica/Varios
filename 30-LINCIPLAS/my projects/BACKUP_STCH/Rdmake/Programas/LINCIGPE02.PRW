Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nOrdem

cDATAde := mv_par01
cDATAate := mv_par02

dbSelectArea("SRA")
cFilTrab   := CriaTrab(NIL,.F.)
cCondicao  := " RA_FILIAL == '"+xFilial("SRA")+"'.And. RA_SITFOLH <> 'D' .AND."
cCondicao  += "Dtos(RA_ADMISSA)>='"+Dtos(cDATAde)+"'.And.Dtos(RA_ADMISSA)<='"+Dtos(cDATAate)+"'"
IndRegua("SRA",cFilTrab,"RA_FILIAL+Dtos(RA_ADMISSA)",,cCondicao,"Filtrando")    // "Selecionando Registros..."
DbGoTop()
SetRegua(RecCount())

_nQuant := 0
While !EOF() 
_nQuan := 0 
_cMES := MONTH(RA_ADMISSA)
_cANO := YEAR(RA_ADMISSA)

While !EOF() .AND. _cMES == MONTH(RA_ADMISSA) .AND. _cANO == YEAR(RA_ADMISSA)
   //ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
   //ณ Verifica o cancelamento pelo usuario...                             ณ
   //ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

   If lAbortPrint
      @nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
      Exit
   Endif

   //ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
   //ณ Impressao do cabecalho do relatorio. . .                            ณ
   //ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

   If nLin > 55 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
      Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
      nLin := 10
   Endif

   
   //"MAT     NOME                            DATA DE ADMISSAO"
   //""       XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX  XXXXXXXXXXXXXXXX       
   // 012345678901234567890123456789012345678901234567890123456789012345678901234567890
   //           1         2         3         4         5         6         7         8
      @nLin,00 PSAY SRA->RA_MAT
      @nLin,08 PSAY SRA->RA_NOME
      @nLin,40 PSAY SRA->RA_ADMISSA
      
      
   _nQuan += 1
   _nQuant += 1
   
   nLin := nLin + 1 // Avanca a linha de impressao
   dbSelectArea("SRA")
   dbSkip() // Avanca o ponteiro do registro no arquivo
End

@nLin,00 PSAY "        Total Mes/Ano: " + Transform(_nQuan, "@E 999")
nLin := nLin + 1
@nLin,00 PSAY Replicate("-",80)
nLin := nLin + 1
End
nLin := nLin + 1
@nLin,00 PSAY "        Total Geral: " + Transform(_nQuant, "@E 999")

           
dbSelectArea("SRA")
retindex()
//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Finaliza a execucao do relatorio...                                 ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SET DEVICE TO SCREEN

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

If aReturn[5]==1
   dbCommitAll()
   SET PRINTER TO
   OurSpool(wnrel)
Endif

MS_FLUSH()

Return  

