				NHORFAT  := (SD2->D2_QUANT / ((3600/SB1->B1_LCICLO) * SB1->B1_LCAV))
				
	        	dbSelectArea("TRB")
     			Reclock("TRB",.T.)
     			Replace TRB_MAQUIN     With   CMAQUINA
