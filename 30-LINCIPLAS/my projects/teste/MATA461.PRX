If ( lCtbOnLine )
	lLct610 := VerPadrao("610")
	lLct620 := VerPadrao("620")
EndIf
If ( lLct610 .Or. lLct620 )
	PRIVATE cLote
	LoteCont("FAT")
	nHdlPrv :=HeadProva(cLote,"MATA460",cUserName,@cArquivo)
EndIf
