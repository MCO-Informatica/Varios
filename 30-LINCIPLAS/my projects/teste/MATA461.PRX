				SD2->D2_IDENTB6 := IIf(SD2->D2_TIPO$"CIP",SD2->D2_IDENTB6,"")
				SD2->D2_EMISSAO := dDataBase				
				//Tratamento para grava o percentual da comissão para TMS
                If nModulo == 43
					If !Empty(SA1->A1_VEND)
						If !Empty(SA1->A1_COMIS)				
							SD2->D2_COMIS1 := SA1->A1_COMIS
