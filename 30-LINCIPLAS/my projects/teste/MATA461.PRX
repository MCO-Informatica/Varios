				aTransp,,,SC6->C6_NUM,SC5->C5_CLIENTE,SC5->C5_LOJACLI,nTotPed,,SC5->C5_TPFRETE)
	MaFisLoad("NF_ESPECIE",SF2->F2_ESPECIE)
