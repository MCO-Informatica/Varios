					(cAliasQRY)->E1_PARCELA,(cAliasQRY)->E1_TIPO,(cAliasQRY)->E1_MOEDA,"R",;
					(cAliasQRY)->E1_CLIENTE,(cAliasQRY)->E1_VENCREA,(cAliasQRY)->E1_LOJA,,  ;
