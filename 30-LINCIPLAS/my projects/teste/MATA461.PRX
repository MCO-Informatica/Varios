IF nModulo == 72
	lExecuta := KEXF720(ThisMark(),ThisInv())
Endif

If ( ExistBlock("M460MARK") )
	lExecuta := ExecBlock("M460MARK",.F.,.F.,{ThisMark(),ThisInv(),cSerie})
