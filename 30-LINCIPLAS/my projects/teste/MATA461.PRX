			If (SF3->(FieldPos ("F3_VFECPMG")))>0 .And. SF2->F2_TIPOCLI=="F"
   			    lGfecpmg  := Iif(ValType(MV_PAR18)<>"N",.F.,(mv_par18==1))    
    			lTitfemg  := Iif(ValType(MV_PAR17)<>"N",.F.,(mv_par17==1))   			    
				nVFecMG   := SF3->F3_VFECPMG
				nMes      := Month(SF2->F2_EMISSAO) 
