			aAdd(aFisGetSC5,{cReferencia,X3_CAMPO,MaFisOrdem(cReferencia)})
		EndIf
		dbSkip()
	EndDo
	aSort(aFisGetSC5,,,{|x,y| x[3]<y[3]})
EndIf


If aRelImp == Nil
	aRelImp := MaFisRelImp("MT100",{"SF2","SD2"})
EndIf
MaFisEnd()

Return( .T. )

