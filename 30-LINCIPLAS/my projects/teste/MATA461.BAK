//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
If ( Substr(cAcesso,51,1) <> "S" )
	dbSelectArea("SF2")
	dbSetOrder(3)
	MsSeek(xFilial("SF2")+Space(Len(SF2->F2_ECF))+"z",.T.)
	dbSkip(-1)
	If ( dDataBase < SF2->F2_EMISSAO )
		Help(" ",1,"DATNF")
		lContinua := .F.
	EndIf
	If lContinua
		MsSeek(xFilial("SF2")+"S"+"z",.T.)
		dbSkip(-1)
		If ( dDataBase < SF2->F2_EMISSAO )
			Help(" ",1,"DATNF")
			lContinua := .F.
		EndIf
	EndIf
EndIf
