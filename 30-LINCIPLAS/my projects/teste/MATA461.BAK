			cFilSC9 += ".And.C9_PEDIDO>='"+MV_PAR05+"'"
			cFilSC9 += ".And.C9_PEDIDO<='"+MV_PAR06+"'"
			cFilSC9 += ".And.C9_CLIENTE>='"+MV_PAR07+"'"
			cFilSC9 += ".And.C9_CLIENTE<='"+MV_PAR08+"'"
			cFilSC9 += ".And.C9_LOJA>='"+MV_PAR09+"'"
			cFilSC9 += ".And.C9_LOJA<='"+MV_PAR10+"'"
