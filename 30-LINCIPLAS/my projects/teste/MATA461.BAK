			cQrySC9 += " AND SC9.C9_PEDIDO<='"+MV_PAR06+"'"
			cQrySC9 += " AND SC9.C9_CLIENTE>='"+MV_PAR07+"'"
			cQrySC9 += " AND SC9.C9_CLIENTE<='"+MV_PAR08+"'"
