User Function DtVldOrc()
Local aArea := GetArea()
Local nDiasLim	:= SuperGetMV("MV_MTLDLIM", ,3)

xdData := dDataBase+1
nDiasLim--
While nDiasLim > 0
	If !GetNewPAR("MV_MTLEFDS",.T.)
		xdData := DataValida(xdData,.t.)
	Endif
	xdData++
	nDiasLim--
Enddo
xdData := DataValida(xdData,.t.)
Return xdData





