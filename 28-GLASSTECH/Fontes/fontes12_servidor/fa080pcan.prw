User Function F080PCAN()

If ProcName( 4 ) = 'FINA080'

	_nResult := TCSPExec('f080pcan', Val( SE5->E5_IDMOVI ), SE5->E5_FILIAL , 'P', SE5->E5_ORIGBD )
       
