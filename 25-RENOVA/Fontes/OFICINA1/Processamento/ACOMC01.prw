Local cTipo 	:=	"Certificados |*_all.pem"
Local cTitulo	:= "Selecione uma nota para Abrir"
Local cDirIni	:= "SERVIDOR"+__PATHGravada
Local cDrive	:= ""
Local cRet		:= ""
Local cDir		:= ""
Local cFile		:= ""
Local cExten	:= ""
Local cGetFile	:= ""

cGetFile	:= cGetFile(cTipo,;
				  cTitulo,;
				  0,cDirIni,.T.,GETF_ONLYSERVER+GETF_NOCHANGEDIR, )

// Separa os componentes
SplitPath( cGetFile, @cDrive, @cDir, @cFile, @cExten )

//Trata variavel de retorno
If !Empty(cFile) 
	cOrig := SUBSTR(cFile,1,LEN(cFile)-4)
EndIf
       			
Return()       