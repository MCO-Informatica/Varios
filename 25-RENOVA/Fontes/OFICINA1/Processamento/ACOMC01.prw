User Function ACOMC01()

Local oBrowse	:= nil

Private cCadastro	:= "Cadastro de Entidades"
Private aRotina		:= MenuDef()

If AliasInDic("Z05")

	oBrowse := FWMBrowse():New()
	
	oBrowse:SetAlias("Z05")
	oBrowse:SetDescription(cCadastro)
	oBrowse:Activate()
Else
	Aviso("Tabela Inexistente","Nใo existe a tabela Z05 para exibi็ใo.",{"OK"},2,"")		
Endif

Return()

