User Function NOMEEMP()


Local cNomeRet  := ""


If Alltrim(SE1->E1_PREFIXO) = "001"

	cNomeRet  := "KENIA INDUSTRIAS TEXTEIS "
	
	Elseif Alltrim(SE1->E1_PREFIXO) = "002"

	cNomeRet  := "ONITEX TINTURARIA  "
	
	Elseif Alltrim(SE1->E1_PREFIXO) = "003"

	cNomeRet  := "KONIA TEXTIL  "

Endif


Return(cNomeRet)