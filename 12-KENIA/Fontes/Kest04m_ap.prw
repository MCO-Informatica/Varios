// Substituido pelo assistente de conversao do AP6 IDE em 12/02/05 ==> Function RunProc
Static Function RunProc()

DbSelectArea("SB1")
DbSetOrder(1)
DbGoTop()

ProcRegua(RecCount())

Do While !Eof() 

    IncProc("Selecionando Produto "+SB1->B1_COD)

    If SB1->B1_COD < MV_PAR01
        DbSkip()
        Loop
    EndIf

    If SB1->B1_COD > MV_PAR02
        Exit
    Endif

    If SB1->B1_TIPO #"PA"
        DbSkip()
        Loop
    EndIf

    DbSelectArea("SB1")
    RecLock("SB1",.f.)
      SB1->B1_RASTRO :=  "L"
    MsUnLock()

    DbSelectArea("SB1")
    DbSkip()
EndDo

Return

