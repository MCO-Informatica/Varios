User Function RAPRSA(cAlias,nReg,nOpcX)

Local oDlgl
// VERIFICA SE O APROVADOR PODE LIBERAR REQUISICAO
IF VERAPROV(SCP->CP_SOLICIT)
//VERIFICA SE JA FOI GERADA PRE REQUISICAO
IF SCP->CP_PREREQU=="S" 
    Aviso("Observacao","Nao e possivel alterar situacao de SA ja requisitada !!!",{"Ok"},,"Atencao:")
ELSE
IF nOpcX==3
  IF SCP->CP_STATSA=="B"
		
    @ 000,000 To 220,400 Dialog Odlg1 Title OemToAnsi("	Libera็ใo de S.A.")
    @ 003,008 To 110,200 Title OemToAnsi("Aprova Solicita็ใo ao Armazem?")
   
    @ 015,015 Say OemToAnsi("N.Requis:") OF Odlgl PIXEL
    @ 015,045 Say SCP->CP_Num Picture "@!" SIZE 60,10 OF Odlgl PIXEL
    @ 025,015 Say OemToAnsi("Produto:") OF Odlgl PIXEL
    @ 025,045 Say SCP->CP_PRODUTO Picture "@!" SIZE 60,10 OF Odlgl PIXEL
    @ 035,015 Say OemToAnsi("Descri็ใo:")OF Odlgl PIXEL
    @ 035,045 Say SCP->CP_DESCRI Picture "@!" SIZE 160,10 OF Odlgl PIXEL
    @ 045,015 Say OemToAnsi("Qtd:") OF Odlgl PIXEL
    @ 045,045 Say SCP->CP_QUANT Picture "@!" SIZE 40,10 OF Odlgl PIXEL
    @ 055,015 Say OemToAnsi("Emitente:")OF Odlgl PIXEL
    @ 055,045 Say SCP->cp_solicit Picture "@!" SIZE 80,10 OF Odlgl PIXEL
    @ 090,030 BMPBUTTON TYPE 1 ACTION OkProc(odlg1)
    @ 090,060 BMPBUTTON TYPE 2 ACTION Finaliza(odlg1)

    Activate Dialog Odlg1 CENTER
  ELSE
    Aviso("Observacao","Requisicao ja foi aprovada !!!",{"Ok"},,"Atencao:")
  ENDIF
ELSEIF nOpcX==4
  IF SCP->CP_STATSA==" "
   		
    @ 000,000 To 220,400 Dialog Odlg1 Title OemToAnsi("Bloqueio de S.A.")
    @ 003,008 To 110,200 Title OemToAnsi("Cancela Libera็ใo?")
   
    @ 015,015 Say OemToAnsi("N.Requis:") OF Odlgl PIXEL
    @ 015,045 Say SCP->CP_Num Picture "@!" SIZE 60,10 OF Odlgl PIXEL
    @ 025,015 Say OemToAnsi("Produto:") OF Odlgl PIXEL
    @ 025,045 Say SCP->CP_PRODUTO Picture "@!" SIZE 60,10 OF Odlgl PIXEL
    @ 035,015 Say OemToAnsi("Descri็ใo:")OF Odlgl PIXEL
    @ 035,045 Say SCP->CP_DESCRI Picture "@!" SIZE 160,10 OF Odlgl PIXEL
    @ 045,015 Say OemToAnsi("Qtd:") OF Odlgl PIXEL
    @ 045,045 Say SCP->CP_QUANT Picture "@!" SIZE 40,10 OF Odlgl PIXEL
    @ 055,015 Say OemToAnsi("Emitente:")OF Odlgl PIXEL
    @ 055,045 Say SCP->cp_solicit Picture "@!" SIZE 80,10 OF Odlgl PIXEL
    @ 090,030 BMPBUTTON TYPE 1 ACTION OkProc(odlg1)
    @ 090,060 BMPBUTTON TYPE 2 ACTION Finaliza(odlg1)

    Activate Dialog Odlg1 CENTER
  ELSE
    Aviso("Observacao","Requisicao ja esta bloqueada !!!",{"Ok"},,"Atencao:")
  Endif
ENDIF
ENDIF
ENDIF
Return(.T.)

