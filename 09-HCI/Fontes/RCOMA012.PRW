User Function RCOMA12TOK()
Local aSaveArea := GetArea()
Local lRet    := .T.
LOCAL cCampo  := M->ZW_CAMPO
dbSelectArea("SX3")
dbSetOrder(2)
IF dbSeek(cCampo)
If SX3->X3_WHEN = "                                                            "
	RecLock("SX3",.F.)
	SX3->X3_WHEN := "U_RMATA030(" + chr(34)+ cCampo + chr(34) + ")"
	MsUnlock()
ENDIF
ENDIF
RestArea(aSaveArea)
Return(lRet)


