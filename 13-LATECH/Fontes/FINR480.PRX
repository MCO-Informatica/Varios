LOCAL cDesc3:=""
LOCAL cString :="SEF"
Local cAux

PRIVATE wnrel
PRIVATE titulo:= "Emiss„o de Cheques em Formul rio Cont¡nuo"
PRIVATE cabec1
PRIVATE cabec2
PRIVATE aReturn := { OemToAnsi("Cheque"), 1, OemToAnsi("Administracao"), 1, 2, 1, "",1 }  //"Cheque"###"Administracao"
PRIVATE nomeprog:="FINR480"
PRIVATE nLastKey:= 0
PRIVATE cPerg   :="FIN480"
PRIVATE lComp   := .T.
PRIVATE lLayOut := .F.
PRIVATE nLinVlr := 0
