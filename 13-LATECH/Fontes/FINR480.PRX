Static Function OrdProcTransf()
	Local nOrdPTransf := 1
	Local aArea := GetArea()
	dbSelectarea("SE5")
	If FieldPos("E5_ProcTra") > 0		
		dbSelectArea("SIX")
		dbSeek("SE5",.T.)
		While SIX->INDICE == "SE5" .And. !Eof()
			If Alltrim(SIX->CHAVE) == "E5_FILIAL+E5_PROCTRA"			    				
				exit
			Endif
			nOrdPTransf++
		    dbSkip()	
		EndDo
		If Alltrim(SIX->CHAVE) <> "E5_FILIAL+E5_PROCTRA"			    				
			nOrdPTransf := 0
		Endif				
	Endif	
	RestArea(aArea)
Return nOrdPTransf
