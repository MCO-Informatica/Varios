Static Function VldVol( cCpo )

	Local lRet 		:= .T.
	Local cDig 		:= AllTrim( &( "cVol" + cCpo ) )
	Local nCntFor	:= 0

	For nCntFor := 1 To Len( cDig )
		If !( SubStr( cDig, nCntFor, 1 ) $ "0123456789" )
			MsgStop( "Informe apenas numeros nos campos de volume" )
			lRet := .F.
			Exit
		Endif
	Next nCntFor

Return lRet

