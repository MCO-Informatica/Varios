Static Function __LoadPedidos(nFlag, nAba)

	Local cQuery := " "
	Local aFieldsQuery := {}
	Local aEstados := Separa(mv_par20,"/") 
	Local aRetorno := {}
	Local aOperacoes := Separa(mv_par22,"/") 
	Local cEstados := " "
	Local cOperacoes := " "
