	nQuantEnt:=0
	aEval(aRetorno,{|x| nQuantEnt+= IIf(x[TOTENT] > 0,1,0)})

	nQuantAEnt:=0
	aEval(aRetorno,{|x| nQuantAEnt+= IIf(x[TOTAESIPI] > 0,1,0)})
	
