	Local xx := 1
	Local cFIlFilial := ""
    Local cTipoHrig  := ""    
    Local cDescTI    := ""
	For i := 1 To Len(aEstados)
		If i == Len(aEstados)
			cEstados += "'"+aEstados[i]+"'"
		Else
			cEstados += "'"+aEstados[i]+"',"
