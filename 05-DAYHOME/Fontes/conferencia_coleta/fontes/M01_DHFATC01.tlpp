			      Case TEMP->C5_DHORIGA == "B"
			         cTipoHrig:= "BONIFICACAO"				         
			    EndCase     
			    */

				Aadd(aRetorno,	{	aRetLegenda[1],; //"Status"
									TEMP->C5_FILIAL,; //"Filial"
									TEMP->C5_EMISSAO,; //"DT Emissao"
