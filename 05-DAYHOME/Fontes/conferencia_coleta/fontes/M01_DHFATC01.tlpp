Static Function __aTitFields(cCampos, cSep)

Local aRetorno := {}
Local aCampos := Separa(cCampos,cSep)
Local nI := 1

Aadd(aRetorno, "Status" )
For nI := 1 To Len(aCampos)
	If !Empty(aCampos[nI])
		dbSelectArea("SX3")
		dbSetOrder(2)
		dbSeek(aCampos[nI])
		Aadd(aRetorno, Alltrim(SX3->X3_TITULO) )
	EndIf
Next nI
Aadd(aRetorno, "" )

Return aRetorno



