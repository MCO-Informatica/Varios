	EndIF
	
	DbSelectArea("SX5")
	SX5->(dbGoTop())
	If DbSeek( xFilial("SX5") + "12" )
		CursorWait()
		While SX5->(!Eof()) .AND. SX5->X5_TABELA == "12"
