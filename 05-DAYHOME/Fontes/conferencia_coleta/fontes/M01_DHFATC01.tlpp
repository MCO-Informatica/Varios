User Function Load01Estado()

	Local cTitulo	:=	"Filtro dos Estados"
	Local MvPar
	Local MvParDef	:=	""

	Private aFil		:= {}
	Private lTipoRet 	:= .T.
	
	cAlias := Alias() 					 		// Salva Alias Anterior
	
	IF lTipoRet
		MvPar:=&(Alltrim(ReadVar()))		 	// Carrega Nome da Variavel do Get em Questao
