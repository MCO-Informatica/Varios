	//Calcula os Totais
	nTotSIpi:=0
	aEval(aRetorno,{|x| nTotSIpi+= x[TOTSEMIPI]})
	
	nTotCIpi:=0
	aEval(aRetorno,{|x| nTotCIpi+= x[TOTSEMIPI] + x[VALORIPI] })

	nTotAESIpi:=0
	aEval(aRetorno,{|x| nTotAESIpi+= x[TOTAESIPI]})

	nTotEnt:=0
