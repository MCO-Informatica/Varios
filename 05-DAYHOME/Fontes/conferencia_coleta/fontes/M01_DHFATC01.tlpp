Static Function __VisualPedido(nAba)

	Local nPos		:= 0
	Local cAlias	:= "SC5"
	Local cFilial 	:= " "
	Local cData		:= " "
	Local cPedido	:= " "
	Local aMvPars	:= {}
	Local bMvPars	:= Nil
	Local yy := 0
	Local cBkpFil	:= ""

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Guarda os dados do parametro³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	For yy := 1 to 18
		cCampo := "mv_par"+STRZERO(yy,2)
		aADD(aMvPars, &cCampo )
	Next yy
	
	nPos := aObjD[nAba]:nAt
	cFilial := aObjD[nAba]:aCols[nPos][2]
	cData := aObjD[nAba]:aCols[nPos][3]
	cPedido	:= aObjD[nAba]:aCols[nPos][18]
                         
    cBkpFil:= cFilAnt
    cFilAnt:= aObjD[nAba]:aCols[nPos][2]
    
	If !Empty(cPedido)
		DbSelectArea("SC5")
		SC5->(dbSetOrder(2))
		If SC5->(dbSeek(aObjD[nAba]:aCols[nPos][2] + DTOS(cData) + cPedido))
	
			a410Visual("SC5",RECNO(),2)
		
		Endif
	Endif
	
	cFilAnt:= cBkpFil
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Retorna os dados do parametro³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	For yy := 1 to 18
		cCampo := "mv_par"+STRZERO(yy,2)
		&cCampo := aMvPars[yy]
	Next yy

Return(.T.)

