	cQuery += " SELECT DISTINCT " + CRLF
	cQuery += "			C5_FILIAL " + CRLF
	cQuery += ",		C5_EMISSAO " + CRLF
	cQuery += ",		C5_NUM " + CRLF
	cQuery += ",		C5_CLIENTE " + CRLF
	cQuery += ",		C5_LOJACLI " + CRLF
	cQuery += ",		A1_NREDUZ " + CRLF
	cQuery += ",		A1_EST " + CRLF
	cQuery += ",		A1_MUN " + CRLF
