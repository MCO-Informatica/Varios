	oPanelCab:Align := CONTROL_ALIGN_TOP

	oPanel01 := TPanel():New(0,0, , oDialog,, .T.,,,, 663, 180, .T.,)
	oPanel01:Align := CONTROL_ALIGN_ALLCLIENT

	oFolder := TFolder():New(0, 0,aTitles,,oPanel01,,,,.T.,.F., 663,0 ,)
