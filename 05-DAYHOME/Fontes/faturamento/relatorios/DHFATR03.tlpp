	MV_PAR02 := SC5->C5_FILIAL
	MV_PAR03 := SC5->C5_NUM
	MV_PAR04 := SC5->C5_NUM
	MV_PAR05 := SC5->C5_CLIENTE
	MV_PAR06 := SC5->C5_CLIENTE
	MV_PAR07 := SC5->C5_EMISSAO
