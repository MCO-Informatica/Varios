	EndIf
    dbSkip()
End

TMPSC5->( dbCloseArea() )

oReport:SetMeter(nCount)

For nY := 1 To Len(aCabecalho)	
	
	If oReport:Cancel()
	 	Exit
  	EndIf 
	
	PrintCabec(aCabecalho,nCol,nY)  
	
	//nLin := 1680       
	//Alert(Str(nLin))
	nTotGeral:=0
		
	For nX := 1 To Len(aCabecalho[nY][37])
	    
		If nLin >= 2720 // 2650
			oReport:EndPage()
