				            {}})					 //37

	    //cProduto := TMPSC5->C6_PRODUTO 
	    //If cProduto == TMPSC5->C6_PRODUTO 
		//	nPos1 := Len(aCabecalho[nPos,3])    
		//	nPos2 := Ascan(aCabecalho[nPos,3,nPos1,36], {|x| Trim(x[1]) == Alltrim(TMPSC5->C6_PRODUTO) })    
		//	If nPos2 == 0
		
