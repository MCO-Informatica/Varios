Static Function  EXPNOTASAI()

Local _cVarTexto		:=  ""
Local _nInc 			:= 	0
Local _nValBrut			:= 	0
Local _nBasIcms			:= 	0
Local _nValIcms			:= 	0
Local _nBasIpi			:= 	0
Local _nValIpi			:= 	0
Local _nBasSol			:= 	0
Local _nValSol			:= 	0
Local _nIsenIcms		:= 	0
Local _nOutrIcms		:= 	0
Local _nIsenIpi			:= 	0
Local _nOutrIpi			:= 	0
Local _tVALCONT	:= 0
Local _tBASEICM	:= 0
Local _tVALICM	:= 0
Local _tBASEIPI	:= 0
Local _tVALIPI	:= 0
Local _tBASERET	:= 0
Local _tICMSRET	:= 0
Local _tISENICM	:= 0
Local _tOUTRICM	:= 0
Local _tISENIPI	:= 0
Local _tOUTRIPI	:= 0
Local _cPerg 				:= 	"EXPORT"
Local _aRegs				:= 	{}
Local _cReg0				:= 	''	//----> header do arquivo
Local _cReg4  			:= 	''	//----> dados cliente
Local _cReg1  			:= 	'' 	//----> cabecalho da nota
Local _cReg2  			:= 	''	//----> registro do produto (itens da nota)
Local _cReg5  			:= 	'' 	//----> dados complementares nota
Local _cReg3				:= 	''	//----> trailer do arquivo

Private _nHdl   		:= fCreate(_cPath4)

//----> MONTA TABELA TEMPORARIA COM AS INFORMACOES NECESSARIAS PARA GERAR O NOTASAI.TXT (NOTAS FISCAIS DE SAIDA)
_cQrySaida:= "SELECT DISTINCT SF2010.F2_DOC, SF2010.F2_SERIE, SF2010.F2_CLIENTE, SF2010.F2_LOJA, SA1010.A1_CGC,  SA1010.A1_PESSOA, SA1010.A1_NREDUZ, SA1010.A1_NOME ,SA1010.A1_EST,  SA1010.A1_INSCR, SA1010.A1_END,  SA1010.A1_CODMUN, SA1010.A1_BAIRRO, "
_cQrySaida+= "SA1010.A1_MUN,  SA1010.A1_CEP,  SA1010.A1_DDD,  SA1010.A1_TEL, SA1010.A1_CONTA,  SA1010.A1_TIPO,  SA1010.A1_SUFRAMA,  SA1010.A1_CODPAIS,  SF2010.F2_EMISSAO, SF2010.F2_DESCONT, "
_cQrySaida+= "SF2010.F2_VALBRUT,  SF2010.F2_BASEICM,  SF2010.F2_VALICM,  SF2010.F2_BASEIPI,  SF2010.F2_VALIPI, SF2010.F2_ICMSRET,  SF2010.F2_BASEISS,  SF2010.F2_VALISS, "
_cQrySaida+= "SF2010.F2_VALIRRF,  SF2010.F2_ESPECI1, SF2010.F2_FRETE,  SF2010.F2_VALCOFI,  SF2010.F2_VALCSLL,  SF2010.F2_VALPIS,  SD2010.D2_COD,  SD2010.D2_QUANT, "
_cQrySaida+= "SD2010.D2_TOTAL,  SD2010.D2_DESCON,  SD2010.D2_BASEICM,  SD2010.D2_VALICM, SD2010.D2_VALIPI,  SD2010.D2_ICMSRET, SD2010.D2_BASEIPI,  SA1010.A1_LOJA,  SA1010.A1_COD, "
_cQrySaida+= "SF2010.F2_SERIE,  SF2010.F2_PBRUTO, SD2010.D2_CF, SD2010.D2_PICM, SD2010.D2_ALIQISS, SD2010.D2_BRICMS, SF2010.F2_VALMERC, SF2010.F2_BRICMS, SF2010.F2_CHVNFE "
_cQrySaida+= "FROM SF2010 INNER JOIN "
_cQrySaida+= "SD2010 ON  SF2010.F2_DOC =  SD2010.D2_DOC AND  SF2010.F2_SERIE =  SD2010.D2_SERIE INNER JOIN "
_cQrySaida+= "SA1010 ON  SF2010.F2_LOJA =  SA1010.A1_LOJA AND  SF2010.F2_CLIENTE =  SA1010.A1_COD "
_cQrySaida+= "WHERE     ( SA1010.D_E_L_E_T_ = '') AND ( SF2010.D_E_L_E_T_ = '') AND ( SD2010.D_E_L_E_T_ = '') AND ( SF2010.F2_EMISSAO >= '"+DTOS(MV_PAR01)+"' AND SF2010.F2_EMISSAO <= '"+DTOS(MV_PAR02)+"') AND SF2010.F2_FILIAL  = '" + xFilial("SF2") + "' AND SD2010.D2_FILIAL = '"+xFilial("SD2")+"' "
_cQrySaida+= "ORDER BY SF2010.F2_DOC, SF2010.F2_SERIE, SF2010.F2_CLIENTE, SF2010.F2_LOJA, SD2010.D2_CF"

dbUseArea(.T., 'TOPCONN', TCGenQry(,,_cQrySaida), 'Query', .F., .F.)
DbSelectArea("Query")
Query->(dbGoTop())
ProcRegua(RecCount())

_nInc		++

//----> GRAVA O HEADER DO ARQUIVO
_cReg0	:= '0'+subs(Dtos(dDataBase),7,2)+subs(Dtos(dDataBase),5,2)+subs(Dtos(dDataBase),1,4)+SUBS(SM0->M0_CGC,1,2)+"."+SUBS(SM0->M0_CGC,3,3)+"."+SUBS(SM0->M0_CGC,6,3)+"/"+SUBS(SM0->M0_CGC,9,4)+"-"+SUBS(SM0->M0_CGC,13,2)+Space(18-Len(SUBS(SM0->M0_CGC,1,2)+"."+SUBS(SM0->M0_CGC,3,3)+"."+SUBS(SM0->M0_CGC,6,3)+"/"+SUBS(SM0->M0_CGC,9,4)+"-"+SUBS(SM0->M0_CGC,13,2)))+'1'+Space(003)+Space(443)+Space(020)+StrZero((_nInc),06)+(CHR(13)+CHR(10))
_cVarTexto := _cReg0

Set Century OFF
If fWrite(_nHdl,_cVarTexto,Len(_cVarTexto)) != Len(_cVarTexto) //Gravacao do arquivo
	If !MsgAlert("Ocorreu um erro na gravacao do arquivo !!","Atencao!")
		Quit
	Endif
Endif

While Eof() == .f.
	
	IncProc("Processando Registro "+StrZero(Recno(),10))
	
	_nInc++
	
	cNota := QUERY->F2_DOC
	
	//----> GRAVA OS DADOS DO DESTINATARIO
	If QUERY->A1_PESSOA$"J"
		_cReg4	 := '4'+IIf(EMPTY(QUERY->A1_CGC),"00.000.000/0000-00",SUBS(QUERY->A1_CGC,1,2)+"."+SUBS(QUERY->A1_CGC,3,3)+"."+SUBS(QUERY->A1_CGC,6,3)+"/"+SUBS(QUERY->A1_CGC,9,4)+"-"+SUBS(QUERY->A1_CGC,13,2)+Space(18-Len(SUBS(QUERY->A1_CGC,1,2)+"."+SUBS(QUERY->A1_CGC,3,3)+"."+SUBS(QUERY->A1_CGC,6,3)+"/"+SUBS(QUERY->A1_CGC,9,4)+"-"+SUBS(QUERY->A1_CGC,13,2))))+SubStr(QUERY->A1_NOME,1,40)+Space(40-Len(SubStr(QUERY->A1_NOME,1,40)))+SubStr(QUERY->A1_NREDUZ,1,20)+Space(20-Len(SubStr(QUERY->A1_NREDUZ,1,20)))+QUERY->A1_EST+Space(02-Len(QUERY->A1_EST))+SubStr(QUERY->A1_INSCR,1,20)+Space(20-Len(SubStr(QUERY->A1_INSCR,1,20)))
	Else
		_cReg4	 := '4'+IIf(EMPTY(QUERY->A1_CGC),"000.000.000-00    ",SUBS(QUERY->A1_CGC,1,3)+"."+SUBS(QUERY->A1_CGC,4,3)+"."+SUBS(QUERY->A1_CGC,7,3)+"-"+SUBS(QUERY->A1_CGC,10,2)+Space(18-Len(SUBS(QUERY->A1_CGC,1,3)+"."+SUBS(QUERY->A1_CGC,4,3)+"."+SUBS(QUERY->A1_CGC,7,3)+"-"+SUBS(QUERY->A1_CGC,10,2))))+SubStr(QUERY->A1_NOME,1,40)+Space(40-Len(SubStr(QUERY->A1_NOME,1,40)))+SubStr(QUERY->A1_NREDUZ,1,20)+Space(20-Len(SubStr(QUERY->A1_NREDUZ,1,20)))+QUERY->A1_EST+Space(02-Len(QUERY->A1_EST))+SubStr(QUERY->A1_INSCR,1,20)+Space(20-Len(SubStr(QUERY->A1_INSCR,1,20)))
	EndiF
	_cReg4	 +=	SubStr(QUERY->A1_END,1,40)+Space(40-Len(SubStr(QUERY->A1_END,1,40)))+SubStr(QUERY->A1_BAIRRO,1,20)+Space(20-Len(SubStr(QUERY->A1_BAIRRO,1,20)))+SubStr(QUERY->A1_MUN,1,20)+Space(20-Len(SubStr(QUERY->A1_MUN,1,20)))+QUERY->A1_CEP+Space(08-Len(QUERY->A1_CEP))+Replicate("0",4)
	_cReg4	 += Replicate("0",3)+Replicate("0",10)+Replicate("0",6)+Replicate("0",3)+Replicate("0",6)+Replicate("0",3)+'1'+Space(01-Len('1'))+Space(18)
	_cReg4	 += Replicate("0",5)+Space(20)+Replicate("0",9)+Replicate("0",5)+'1'+Space(01-Len('1'))+Space(206)+Space(05)+StrZero((_nInc),06)
	_cReg4	 +=(CHR(13)+CHR(10))
	_cVarTexto := _cReg4
	
	Set Century OFF
	If fWrite(_nHdl,_cVarTexto,Len(_cVarTexto)) != Len(_cVarTexto) //Gravacao do arquivo
		If !MsgAlert("Ocorreu um erro na gravacao do arquivo !!","Atencao!")
			Quit
		Endif
	Endif
	
	
	cCFOP:= ""
	
	While Eof() == .f. .And. QUERY->F2_DOC == cNota
		
		//If QUERY->D2_CF <> cCFOP
			
			_nInc++
			
			_cDoc		:=	QUERY->F2_DOC
			_cSer		:=	QUERY->F2_SERIE
			_cCliente	:= 	QUERY->F2_CLIENTE
			_cLoja		:=	QUERY->F2_LOJA
			_cCF		:=	QUERY->D2_CF
			_cData		:=	QUERY->F2_EMISSAO
			_cCHVNFE	:=	QUERY->F2_CHVNFE
			
			_cQryCFOP:= "SELECT SD2010.D2_CF, SD2010.D2_DOC, SD2010.D2_SERIE, SD2010.D2_CLIENTE, SD2010.D2_LOJA, SD2010.D2_PICM, SUM(SD2010.D2_TOTAL) D2_TOTAL, SUM(SD2010.D2_BRICMS) D2_BRICMS, SUM(SD2010.D2_BASEICM) D2_BASEICM, SUM(SD2010.D2_VALICM) D2_VALICM, SUM(SD2010.D2_VALIPI) D2_VALIPI, SUM(SD2010.D2_ICMSRET) D2_ICMSRET, SUM(SD2010.D2_BASEIPI) D2_BASEIPI, SUM(SD2010.D2_TOTAL+SD2010.D2_VALIPI+SD2010.D2_ICMSRET+SD2010.D2_DESPESA+SD2010.D2_SEGURO+SD2010.D2_VALFRE) D2_VALBRUT FROM SD2010 "
			_cQryCFOP+= "WHERE ( SD2010.D_E_L_E_T_ = '') AND ( SD2010.D2_EMISSAO >= '"+DTOS(MV_PAR01)+"' AND SD2010.D2_EMISSAO <= '"+DTOS(MV_PAR02)+"') AND SD2010.D2_FILIAL  = '"+xFilial("SD2")+"'  "
			_cQryCFOP+= "AND SD2010.D2_DOC = '"+_cDoc+"' AND SD2010.D2_SERIE = '"+_cSer+"' AND SD2010.D2_CLIENTE = '"+_cCliente+"' AND SD2010.D2_LOJA = '"+_cLoja+"' AND SD2010.D2_CF = '"+_cCF+"'"
			_cQryCFOP+= "GROUP BY SD2010.D2_DOC, SD2010.D2_SERIE, SD2010.D2_CLIENTE, SD2010.D2_LOJA, SD2010.D2_CF, SD2010.D2_PICM "
			_cQryCFOP+= "ORDER BY SD2010.D2_DOC, SD2010.D2_SERIE, SD2010.D2_CLIENTE, SD2010.D2_LOJA, SD2010.D2_CF "
			
			dbUseArea(.T., 'TOPCONN', TCGenQry(,,_cQryCFOP), 'CFOP', .F., .F.)
			DbSelectArea("CFOP")
			
			
			dbSelectArea("SF3")
			dbSetOrder(1)
			//dbSeek(xFilial("SF3")+_cData+_cDoc+_cSer+_cCliente+_cLoja+_cCF,.f.)
			dbSeek(xFilial("SF3")+_cData+_cDoc+_cSer+_cCliente+_cLoja,.f.)
			
			//While Eof() = .f. .And. (Dtos(SF3->F3_ENTRADA)+SF3->F3_NFISCAL+SF3->F3_SERIE+SF3->F3_CLIEFOR+SF3->F3_LOJA+SF3->F3_CFO) == _cData+_cDoc+_cSer+_cCliente+_cLoja+_cCF
			While Eof() = .f. .And. (Dtos(SF3->F3_ENTRADA)+SF3->F3_NFISCAL+SF3->F3_SERIE+SF3->F3_CLIEFOR+SF3->F3_LOJA) == _cData+_cDoc+_cSer+_cCliente+_cLoja
				
				_tVALCONT	+= SF3->F3_VALCONT
				_tBASEICM	+= SF3->F3_BASEICM
				_tVALICM	+= SF3->F3_VALICM
				_tBASEIPI	+= SF3->F3_BASEIPI
				_tVALIPI	+= SF3->F3_VALIPI
				_tBASERET	+= SF3->F3_BASERET
				_tICMSRET	+= SF3->F3_ICMSRET
				_tISENICM	+= SF3->F3_ISENICM
				_tOUTRICM	+= SF3->F3_OUTRICM
				_tISENIPI	+= SF3->F3_ISENIPI
				_tOUTRIPI	+= SF3->F3_OUTRIPI 
				_cCFOPSF3	:= SF3->F3_CFO    
				_tALIQICM	:= SF3->F3_ALIQICM
				
				dbSelectArea("SF3")
				dbSkip()
			EndDo
			
