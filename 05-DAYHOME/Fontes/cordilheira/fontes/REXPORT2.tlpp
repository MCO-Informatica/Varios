			EndIf                                                                                                                                                                                                                                                              
			_cReg1  += Replicate("0",12)+Replicate("0",12)+Replicate("0",12)+Replicate("0",12)+StrZero((QUERY->F1_BASEICM*100),12)+Space(12-Len(StrZero((QUERY->F1_BASEICM*100),12)))+StrZero((QUERY->D1_PICM*100),04)+Space(04-Len(StrZero((QUERY->D1_PICM*100),04)))
