Static Function EXPNOTAENT()

Local _cVarTexto 			:=  ""
Local _nInc 				:= 0
Local _nValBrut				:= 0
Local _nBasIcms				:= 0
Local _nValIcms				:= 0
Local _nBasIpi				:= 0
