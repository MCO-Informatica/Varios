Local _cPerg 				:= "EXPORT"
Local _aRegs				:= {}
Private _nHdl   			:= fCreate(_cPath5)


_cQryEntra:= "SELECT DISTINCT SF1010.F1_TIPO, SF1010.F1_DOC, SF1010.F1_SERIE, SF1010.F1_FORNECE, SF1010.F1_LOJA,  SF1010.F1_DTDIGIT, SF1010.F1_EMISSAO, SF1010.F1_DESCONT, "
