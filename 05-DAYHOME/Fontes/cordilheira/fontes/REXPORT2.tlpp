			_cReg1	+= StrZero((QUERY->F1_BASEIPI*100),12)+Space(12-Len(StrZero((QUERY->F1_BASEIPI*100),12)))+StrZero((QUERY->F1_VALIPI*100),12)+Space(12-Len(StrZero((QUERY->F1_VALIPI*100),12)))+StrZero((SF3->F3_ISENIPI*100),12)+Space(12-Len(StrZero((SF3->F3_ISENIPI*100),12)))
