	cCFOP := ""
	_nInc ++
	
			DbSelectArea("QUERY")
			
			//----> GRAVA O CABECALHO DA NOTA
			_cReg1	:= '1'+SubStr(QUERY->F1_DTDIGIT,7,2)+SubStr(QUERY->F1_DTDIGIT,5,2)+SubStr(QUERY->F1_DTDIGIT,1,4)+SubStr(QUERY->F1_DOC,1,6)+Space(06-Len(SubStr(QUERY->F1_DOC,1,6)))+SubStr(QUERY->F1_EMISSAO,7,2)+SubStr(QUERY->F1_EMISSAO,5,2)+SubStr(QUERY->F1_EMISSAO,1,4)+Space(08-Len(SubStr(QUERY->F1_EMISSAO,7,2)+SubStr(QUERY->F1_EMISSAO,5,2)+SubStr(QUERY->F1_EMISSAO,1,4)))+'55'+substr(QUERY->F1_SERIE,1,3)+Space(03-Len(substr(QUERY->F1_SERIE,1,3)))+Space(03)
			//If FORNEC->A2_TIPO$"JX"
			If LEN(ALLTRIM(FORNEC->A2_CGC)) > 11
				_cReg1	+= SubStr(QUERY->D1_CF,1,4)+Space(04-Len(SubStr(QUERY->D1_CF,1,4)))+'01'+Replicate("0",2)+Replicate("0",2)+IIf(EMPTY(FORNEC->A2_CGC),"00.000.000/0000-00",SUBS(FORNEC->A2_CGC,1,2)+"."+SUBS(FORNEC->A2_CGC,3,3)+"."+SUBS(FORNEC->A2_CGC,6,3)+"/"+SUBS(FORNEC->A2_CGC,9,4)+"-"+SUBS(FORNEC->A2_CGC,13,2)+Space(18-Len(SUBS(FORNEC->A2_CGC,1,2)+"."+SUBS(FORNEC->A2_CGC,3,3)+"."+SUBS(FORNEC->A2_CGC,6,3)+"/"+SUBS(FORNEC->A2_CGC,9,4)+"-"+SUBS(FORNEC->A2_CGC,13,2))))+StrZero((QUERY->F1_VALBRUT*100),12)+Space(12-Len(StrZero((QUERY->F1_VALBRUT*100),12)))
			Else
