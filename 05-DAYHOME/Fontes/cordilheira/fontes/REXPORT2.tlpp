	EndiF
	
	//_cReg4	 := '4'+IIf(EMPTY(FORNEC->A2_CGC),"00.000.000/0000-00",SUBS(FORNEC->A2_CGC,1,2)+"."+SUBS(FORNEC->A2_CGC,3,3)+"."+SUBS(FORNEC->A2_CGC,6,3)+"/"+SUBS(FORNEC->A2_CGC,9,4)+"-"+SUBS(FORNEC->A2_CGC,13,2)+Space(18-Len(SUBS(FORNEC->A2_CGC,1,2)+"."+SUBS(FORNEC->A2_CGC,3,3)+"."+SUBS(FORNEC->A2_CGC,6,3)+"/"+SUBS(FORNEC->A2_CGC,9,4)+"-"+SUBS(FORNEC->A2_CGC,13,2))))+SubStr(FORNEC->A2_NOME,1,40)+Space(40-Len(SubStr(FORNEC->A2_NOME,1,40)))+SubStr(FORNEC->A2_NREDUZ,1,20)+Space(20-Len(SubStr(FORNEC->A2_NREDUZ,1,20)))+FORNEC->A2_EST+Space(02-Len(FORNEC->A2_EST))+SubStr(FORNEC->A2_INSCR,1,20)+Space(20-Len(SubStr(FORNEC->A2_INSCR,1,20)))
	_cReg4	 +=	SubStr(FORNEC->A2_END,1,40)+Space(40-Len(SubStr(FORNEC->A2_END,1,40)))+SubStr(FORNEC->A2_BAIRRO,1,20)+Space(20-Len(SubStr(FORNEC->A2_BAIRRO,1,20)))
	_cReg4	 += Replicate("0",20)//IIF(FORNEC->A2_MUN <> '',SubStr(FORNEC->A2_MUN,1,20)+Space(20-Len(SubStr(FORNEC->A2_MUN,1,20))),Replicate("0",20))
	_cReg4	 += IIF(FORNEC->A2_CEP <> '',FORNEC->A2_CEP+Space(08-Len(FORNEC->A2_CEP)),Replicate("0",8))
	_cReg4	 +=	Replicate("0",4)//IIF(FORNEC->A2_CODMUN <> '',SubStr(FORNEC->A2_CODMUN,1,4)+Space(04-Len(SubStr(FORNEC->A2_CODMUN,1,4))),Replicate("0",4))
	_cReg4	 += Replicate("0",3)//IIF(FORNEC->A2_DDD <> '',FORNEC->A2_DDD+Space(03-Len(FORNEC->A2_DDD)),Replicate("0",3))
	_cReg4	 +=	Replicate("0",10)+Replicate("0",6)+Replicate("0",3)+Replicate("0",6)+Replicate("0",3)+'1'+Space(01-Len('1'))+Space(18)
	_cReg4	 += Replicate("0",5)+Space(20)+Replicate("0",9)+Replicate("0",5)+'1'+Space(01-Len('1'))+Space(206)+Space(05)+StrZero((_nInc),06)
	_cReg4	 +=(CHR(13)+CHR(10))
	_cVarTexto := _cReg4
	
	Set Century OFF
	If fWrite(_nHdl,_cVarTexto,Len(_cVarTexto)) != Len(_cVarTexto) //Gravacao do arquivo
		If !MsgAlert("Ocorreu um erro na gravacao do arquivo !!","Atencao!")
			Quit
		Endif
