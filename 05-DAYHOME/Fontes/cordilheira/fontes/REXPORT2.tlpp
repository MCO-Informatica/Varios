_cQryEntra+= "AND SFT010.D_E_L_E_T_ = '' AND SFT010.FT_DTCANC = '' "
_cQryEntra+= "WHERE     (SF1010.D_E_L_E_T_ = '') AND ( SD1010.D_E_L_E_T_ = '') AND ( SF1010.F1_DTDIGIT >= '"+DTOS(MV_PAR01)+"' AND SF1010.F1_DTDIGIT <= '"+DTOS(MV_PAR02)+"') AND  SF1010.F1_FILIAL  = '" + xFilial("SF1") + "' AND SD1010.D1_FILIAL = '"+xFilial("SD1")+"' "
_cQryEntra+= "ORDER BY SF1010.F1_DOC, SF1010.F1_SERIE, SF1010.F1_FORNECE, SF1010.F1_LOJA, SD1010.D1_CF "


dbUseArea(.T., 'TOPCONN', TCGenQry(,,_cQryEntra), 'Query', .F., .F.)
