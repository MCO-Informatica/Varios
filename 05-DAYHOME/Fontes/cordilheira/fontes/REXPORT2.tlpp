_cQryEntra+= "SF1010.F1_SERIE,  SF1010.F1_PBRUTO, SD1010.D1_CF, SD1010.D1_PICM, SD1010.D1_ALIQISS, SD1010.D1_BRICMS, SF1010.F1_VALMERC, SF1010.F1_BRICMS, FT_CLASFIS, FT_CTIPI, FT_CSTPIS, FT_CSTCOF, F1_CHVNFE "
_cQryEntra+= "FROM SF1010 INNER JOIN "
_cQryEntra+= "SD1010 ON  SF1010.F1_DOC =  SD1010.D1_DOC AND  SF1010.F1_SERIE =  SD1010.D1_SERIE AND SF1010.F1_FORNECE = SD1010.D1_FORNECE AND SF1010.F1_LOJA = SD1010.D1_LOJA AND SF1010.F1_TIPO = SD1010.D1_TIPO "
_cQryEntra+= "INNER JOIN SFT010 ON " 
