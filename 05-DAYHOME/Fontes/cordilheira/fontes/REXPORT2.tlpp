			_cReg1	+= SubStr(IIF(QUERY->F1_ESPECIE$"SPED ","NFE  ",QUERY->F1_ESPECIE),1,5)+Space(05-Len(SubStr(QUERY->F1_ESPECIE,1,5)))+'N'+Replicate("0",04)+Replicate("0",08)+Replicate("0",12)+Replicate("0",04)+StrZero((QUERY->D1_VALISS*100),12)+Space(12-Len(StrZero((QUERY->D1_VALISS*100),12)))+Replicate("0",12)+StrZero((QUERY->D1_VALIRR*100),12)+Space(12-Len(StrZero((QUERY->D1_VALIRR*100),12)))+Replicate("0",12)+Replicate("0",12)+Replicate("0",12)+SubStr(QUERY->F1_DTDIGIT,7,2)+SubStr(QUERY->F1_DTDIGIT,5,2)+SubStr(QUERY->F1_DTDIGIT,1,4)+Space(08-Len(SubStr(QUERY->F1_DTDIGIT,7,2)+SubStr(QUERY->F1_DTDIGIT,5,2)+SubStr(QUERY->F1_DTDIGIT,1,4)))
			_cReg1	+= Replicate("0",04)+Replicate(" ",06)+Replicate(" ",18)+Replicate(" ",12)+Replicate(" ",12)+Replicate(" ",04)+Replicate(" ",18)+Replicate(" ",05)+StrZero((_nInc),06)+QUERY->F1_DOC
			_cReg1	+=(CHR(13)+CHR(10))
