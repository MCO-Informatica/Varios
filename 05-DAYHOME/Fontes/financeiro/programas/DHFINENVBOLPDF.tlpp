LOCAL i := 0

aBmp 	:= SuperGetMv("DH_LGITAU",.F.,"\SYSTEM\ITAU.BMP")
aBmp2 	:= SuperGetMv("DH_LOGBOL",.F.,"\SYSTEM\LOGO.BMP")

oFont2n := TFont():New("Times New Roman",,12,,.T.,,,,,.F. )
oFont8  := TFont():New("Arial",9,10 ,.T.,.F.,5,.T.,5,.T.,.F.)
